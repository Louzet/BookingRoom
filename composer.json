{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "adldap2/adldap2": "^9.1",
        "behat/transliterator": "^1.2",
        "cocur/slugify": "^3.1",
        "eonasdan/bootstrap-datetimepicker": "^4.17",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "ldaptools/ldaptools": "^0.25.2",
        "phpmailer/phpmailer": "^6.0",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "smalot/ldap": "^0.4.0",
        "snc/redis-bundle": "^2.1",
        "sonata-project/admin-bundle": "^3.45",
        "sonata-project/doctrine-orm-admin-bundle": "^3.7",
        "sonata-project/exporter": "1.11.*",
        "symfony/asset": "4.2.*",
        "symfony/cache": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/ldap": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "1.0.*",
        "symfony/process": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "1.0.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/workflow": "4.2.*",
        "symfony/yaml": "4.2.*",
        "toiba/fullcalendar-bundle": "^6.2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^7.5",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "4.2.*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
